      aftermath: "\u0421\u0435\u043C\u044C\u044F \u0438\u0434\u0435\u0442 \u043A
        \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u0439 \u043C\u0430\u0448\u0438\u043D\u0435"
      actions: []
    choiceRight:
      text: "\u041D\u0443 \u0441\u0430\u0434\u0438\u0441\u044C \u043C\u0443\u0436\u0438\u043A"
