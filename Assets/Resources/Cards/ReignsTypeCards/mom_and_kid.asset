      - type: 1
        data:
          rid: 2448774329981468742
  - id: 1
    character: 21
    text: "\u041F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430, \u043E\u0447\u0435\u043D\u044C
      \u043D\u0443\u0436\u043D\u0430 \u0432\u0430\u0448\u0430 \u043F\u043E\u043C\u043E\u0449\u044C,
      \u043D\u0430\u0441 \u0431\u0435\u0437 \u043C\u0430\u0448\u0438\u043D\u044B
      \u043D\u0435 \u043F\u0443\u0441\u0442\u0438\u043B\u0438, \u043C\u044B \u0437\u0430\u043F\u043B\u0430\u0442\u0438\u043C"
    choiceLeft:
      text: "\u041D\u0435\u0442 "
      aftermath: "\u0421\u0435\u043C\u044C\u044F \u0438\u0434\u0435\u0442 \u043A
        \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u0439 \u043C\u0430\u0448\u0438\u043D\u0435"
      actions: []
    choiceRight:
      text: "\u0423 \u043C\u0435\u043D\u044F \u043C\u0430\u043B\u043E \u043C\u0435\u0441\u0442\u0430"
