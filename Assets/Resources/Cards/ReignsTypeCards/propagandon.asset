  dialogs:
  - id: 0
    character: 9
    text: "\u0417\u0434\u0440\u0430\u0432\u0441\u0442\u0432\u0443\u0439\u0442\u0435!
      \u042F \u0436\u0443\u0440\u043D\u0430\u043B\u0438\u0441\u0442, \u043C\u043E\u0436\u043D\u043E
      \u0437\u0430\u0434\u0430\u0442\u044C \u043F\u0430\u0440\u0443 \u0432\u043E\u043F\u0440\u043E\u0441\u043E\u0432?"
    choiceLeft:
      text: "\u041D\u0435\u0442"
      aftermath: 
      actions:
      - type: 1
        data:
          rid: 418072007160889515
    choiceRight:
      text: "\u0414\u0430, \u043A\u043E\u043D\u0435\u0447\u043D\u043E"
      aftermath: 
      actions:
      - type: 1
        data:
          rid: 418072007160889517
  - id: 1
    character: 9
    text: "\u041F\u043E\u0447\u0435\u043C\u0443 \u0432\u044B \u0443\u0435\u0437\u0436\u0430\u0435\u0442\u0435?
      \u0414\u0443\u043C\u0430\u0435\u0442\u0435 \u0442\u0430\u043C \u0431\u0443\u0434\u0435\u0442
      \u043B\u0443\u0447\u0448\u0435, \u0447\u0435\u043C \u0434\u043E\u043C\u0430?"
    choiceLeft:
      text: "\u0422\u043E\u0447\u043D\u043E \u043D\u0435 \u0445\u0443\u0436\u0435,
        \u0447\u0435\u043C \u0432 \u0420\u043E\u0441\u0441\u0438\u0438"
      aftermath: "\u0410\u0433\u0430, \"\u0445\u0443\u0436\u0435, \u0447\u0435\u043C
        \u0432 \u0420\u043E\u0441\u0441\u0438\u0438\" - \u0442\u0430\u043A \u0438
        \u0437\u0430\u043F\u0438\u0448\u0435\u043C"
      actions:
      - type: 1
        data:
          rid: 418072007160889521
    choiceRight:
      text: "\u041F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044C\u0441\u0442\u0432\u0430
        \u043D\u0430\u0448\u0435 \u043B\u0443\u0447\u0448\u0435 \u0441\u043F\u0440\u043E\u0441\u0438\u0442\u0435"
      aftermath: "\"\u041F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044C\u0441\u0442\u0432\u043E
        \u043D\u0430\u0448\u0435 \u043B\u0443\u0447\u0448\u0435\" - \u044D\u0442\u043E
        \u0445\u043E\u0440\u043E\u0448\u043E"
      actions:
      - type: 1
        data:
          rid: 418072007160889519
  - id: 2
    character: 9
    text: "\u041F\u0440\u043E\u043A\u043E\u043C\u043C\u0435\u043D\u0442\u0438\u0440\u0443\u0439\u0442\u0435
      \u043E\u0442\u0440\u0446\u0430\u0442\u0435\u043B\u044C\u043D\u044B\u0439 \u0440\u043E\u0441\u0442
      \u043B\u044E\u0434\u0435\u0439 \u043F\u0440\u0438\u0437\u044B\u0432\u043D\u043E\u0433\u043E
      \u0432\u043E\u0437\u0440\u0430\u0441\u0442\u0430. \u041D\u0435 \u0431\u043E\u0438\u0442\u0435\u0441\u044C
      \u0441\u0438\u043C\u043C\u0435\u0442\u0440\u0438\u0447\u043D\u043E\u0433\u043E
      \u043E\u0442\u0432\u0435\u0442\u0430 \u0441\u043E \u0441\u0442\u043E\u0440\u043E\u043D\u044B
      \u0420\u043E\u0441\u0441\u0438\u0438?"
    choiceLeft:
      text: "\u0421\u0430\u043C\u0438-\u0442\u043E \u043F\u043E\u043D\u044F\u043B\u0438,
        \u0447\u0442\u043E \u0441\u043A\u0430\u0437\u0430\u043B\u0438?"
      aftermath: 
      actions: []
    choiceRight:
      text: "\u0412\u0441\u0435\u043C \u043D\u0430\u043C \u0441\u0442\u0440\u0430\u0448\u043D\u043E.
        \u041A\u043E\u0433\u0434\u0430 \u0432\u0441\u0451 \u0437\u0430\u043A\u043E\u043D\u0447\u0438\u0442\u0441\u044F,
        \u043C\u044B \u0445\u043E\u0442\u0438\u043C \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F
        \u043D\u0430 \u0420\u043E\u0434\u0438\u043D\u0443. \u0412 \u044D\u0442\u043E\u0439
        \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438 \u043D\u0435 \u043C\u044B
        \u043F\u0440\u0435\u0434\u0430\u0442\u0435\u043B\u0438"
      aftermath: "\u0422\u0430\u043A-\u0442\u0430\u043A, \"\u041D\u0430\u043C \u0441\u0442\u0440\u0430\u0448\u043D\u043E.
        \u041C\u044B \u0445\u043E\u0442\u0438\u043C \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F
        \u043D\u0430 \u0420\u043E\u0434\u0438\u043D\u0443. \u041C\u044B \u043F\u0440\u0435\u0434\u0430\u0442\u0435\u043B\u0438\"
        - \u0432\u0441\u0451 \u044F\u0441\u043D\u043E. \u042F \u0443\u0437\u043D\u0430\u043B
        \u0432\u0441\u0451, \u0447\u0442\u043E \u0445\u043E\u0442\u0435\u043B. \u0421\u043F\u0430\u0441\u0438\u0431\u043E"
